#### Global Configuration Options ####
option domain-name "domain.com";
default-lease-time 900;
max-lease-time 1800;
option domain-name-servers dc1.domain.com;
ddns-update-style ad-hoc;
authoritative;
allow bootp;

#### Global PXE Configuration Options ####
option space PXE;
option PXE.mtftp-ip code 1 = ip-address;
option PXE.mtftp-cport code 2 = unsigned integer 16;
option PXE.stftp-cport code 3 = unsigned integer 16;
option PXE.mtftp-tmout code 4 = unsigned integer 8;
option PXE.mtftp-delay code 5 = unsigned integer 8;
option PXE.discovery-control code 6 = unsigned integer 8;
option PXE.discovery-mcast-addr code 7 = ip-address;

#### DNSSEC Key Definitions ####
key primarykey {
     algorithm HMAC-MD5;
     secret pRP5FapFoJ95JEL06sv4PQ==;
}

#### DNS Zone Definitions ####
zone domain.com {
     primary primary;
     key primarykey;
}
zone 2.15.168.192.in-addr.arpa {
     primary primary;
     key primarykey;
}

#### Failover configuration ####
failover peer "dc1" {
     primary;
     address 192.168.15.2;
     port 519;
     peer address 192.168.15.1;
     peer port 520;
     max-response-delay 30;
     max-unacked-updates 10;
     mclt 5;
     split 128;
     load balance max seconds 30;
}

#### Class Definitions ####

### 'match-intel' Class Definition ###
class "match-intel" {
     match if substring ( option dhcp-client-identifier, 1, 3 ) = "abc";
     option domain-name scl;
     option domain-name-servers 192.168.15.3;
     option subnet-mask 255.255.255.128;
}

### 'match-rf' Class Definition ###
class "match-rf" {
     option time-servers time.domain.com;
}

### 'match-pc-groups' Class Definition ###
class "match-pc-groups" {
     option root-path "smb://server.domain.com/test-share";
}

### 'match-mac-group' Class Definition ###
class "match-mac-group" {
     option domain-name scl;
     match if substring ( option dhcp-client-identifier, 1, 3 ) = "0e0";
     option broadcast-address 255.255.255.128;
}

### 'match-intel-adapters' Class Definition ###
class "match-intel-adapters" {
     option domain-name-servers domain.com, domain1.com;
     option domain-name sub;
     match if substring ( option dhcp-client-identifier, 1, 3 ) = "0e0";
}

#### Shared Network Definitions ####

### Shared Network 'MySharedNetwork' ###
shared-network MySharedNetwork {

     ## 'mac_lib_mmc' Subnet Definition ##
     subnet 192.168.14.0 netmask 255.255.255.0 {
          option domain-name-servers 192.168.115.10, 192.168.201.10;
          option routers 192.168.14.1;
          range 192.168.14.1 192.168.14.100;
          option ip-forwarding off;
     }

     ## 'mac_mmc_staff' Subnet Definition ##
     subnet 192.168.16.0 netmask 255.255.255.128 {
          option domain-name-servers 192.168.115.10, 192.168.201.10;
          option routers 192.168.16.1;
          option ip-forwarding off;
     }

     ## 'mac_union_lvl_2' Subnet Definition ##
     subnet 192.168.211.192 netmask 255.255.255.192 {
          option domain-name-servers 192.168.115.10, 192.168.201.10;
          option routers 192.168.211.193;
          option ip-forwarding off;
     }

     ## 'mac_lib_staff' Subnet Definition ##
     subnet 192.168.12.0 netmask 255.255.255.0 {
          option domain-name-servers 192.168.115.10, 192.168.201.10;
          option routers 192.168.12.1;
     }

     ## 'mac_mmc_servers' Subnet Definition ##
     subnet 192.168.17.128 netmask 255.255.255.224 {
          option domain-name-servers 192.168.115.10, 192.168.201.10;
          option routers 192.168.17.129;
          option ip-forwarding off;
     }
}

#### Subnet Definitions ####

### 'Vlan-022' Subnet Definition ###
subnet 192.168.241.0 netmask 255.255.255.0 {
     option domain-name-servers 192.168.15.2, 192.168.18.2;
     option routers 192.168.241.1;
     option ip-forwarding off;
}

### 'Vlan-029' Subnet Definition ###
subnet 192.168.15.0 netmask 255.255.255.0 {
     option domain-name-servers 192.168.15.2, 192.168.18.2;
     option routers 192.168.15.1;
     option ip-forwarding off;
}

### 'Vlan-151' Subnet Definition ###
subnet 192.168.18.0 netmask 255.255.255.0 {
     option domain-name-servers 192.168.18.2, 192.168.15.2;
     option routers 192.168.18.1;
}

### 'Vlan-461' Subnet Definition ###
subnet 192.168.16.128 netmask 255.255.255.128 {
     option domain-name-servers 192.168.16.130, 192.168.15.2;
     option routers 192.168.16.129;
     option ip-forwarding off;
     ## Pool 'staff' ##
     pool {
          option domain-name-servers 192.168.16.130, 192.168.16.2;
          option routers 192.168.16.129;
          range 192.168.15.110 192.168.16.255;
          option ip-forwarding on;
          option broadcast-address 192.168.16.255;
          option ntp-servers time.domain.com;
          default-lease-time 1800;
          min-lease-time 700;
          max-lease-time 3200;
          allow members of "match-intel-adapters";
     }
}

### 'Vlan-031' Subnet Definition ###
subnet 192.168.8.0 netmask 255.255.255.0 {
     option domain-name-servers 192.168.15.2, 192.168.18.2;
     option routers 192.168.8.1;
     option ip-forwarding off;
}

